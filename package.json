{
  "name": "@cloudcmd/create-element",
  "version": "1.1.1",
  "description": "create dom element",
  "homepage": "http://github.com/cloudcmd/create-element",
  "repository": {
    "type": "git",
    "url": "git://github.com/cloudcmd/create-element.git"
  },
  "main": "legacy/create-element.js",
  "scripts": {
    "watch": "nodemon --watch lib --exec",
    "watch:client": "redrun compile -- --watch",
    "watch:test": "npm run watch -- npm test",
    "watch:lint": "npm run watch -- 'npm run lint'",
    "watch:lint:js": "npm run watch -- \"run lint:js\"",
    "watch:coverage": "redrun watch -- redrun coverage",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "compile": "babel -d legacy lib",
    "build": "redrun clean build:*",
    "build:js": "redrun compile legacy:*",
    "legacy:index": "echo \"module.exports = require('./create-element');\" > legacy/index.js",
    "clean": "rimraf dist legacy",
    "wisdom": "npm run build",
    "lint:js": "eslint lib",
    "lint": "redrun lint:*",
    "test": "tape 'lib/**/*.spec.js'",
    "test:update": "UPDATE_FIXTURE=1 redrun test"
  },
  "keywords": [
    "create",
    "element",
    "DOM"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloudcmd/create-element/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "auto-globals": "^1.0.0",
    "autoprefixer": "^9.1.0",
    "babel-loader": "^8.0.0",
    "clean-css-loader": "^1.0.1",
    "coveralls": "^3.0.0",
    "css-loader": "^3.2.0",
    "css-modules-require-hook": "^4.2.3",
    "eslint": "^5.0.0",
    "mock-require": "^3.0.2",
    "nodemon": "^1.9.2",
    "nyc": "^13.0.1",
    "redrun": "^7.0.2",
    "rimraf": "^2.5.3",
    "style-loader": "^1.0.0",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.0.0",
    "tape": "^4.6.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^3.0.8",
    "wraptile": "^3.0.0"
  },
  "dependencies": {
    "currify": "^3.0.0",
    "fullstore": "^2.0.2"
  }
}
